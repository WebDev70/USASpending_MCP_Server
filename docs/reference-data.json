  "unique_entity_identifier": {
    "description": "UEI - Unique Entity Identifier for federal contractors and vendors",
    "what_is_uei": "A unique 13-character alphanumeric code assigned to all federal contractors, grantees, and vendors by the federal government.",
    "format": "13 characters: mix of numbers and uppercase letters (e.g., NWM1JWVDA853, G2NLP3NLX5Q7)",
    "replaced": "DUNS number (though both may exist for legacy reasons)",
    "replaces_what": "UEI replaced the Data Universal Numbering System (DUNS) number as the primary federal vendor identifier",
    "how_to_find": [
      "Ask the vendor directly - they know their UEI",
      "Search USASpending.gov for the company name and get UEI from results",
      "Use search_federal_awards with company name, then extract UEI from results",
      "Use get_vendor_by_uei if you already have the UEI"
    ],
    "search_methods": {
      "by_uei": {
        "tool": "get_vendor_by_uei",
        "description": "Fastest method if you have the exact UEI",
        "example": "get_vendor_by_uei('NWM1JWVDA853')"
      },
      "by_keyword": {
        "tool": "search_federal_awards",
        "description": "Works with company name, type, location, etc.",
        "example": "search_federal_awards('AM General LLC')"
      },
      "by_uei_keyword_search": {
        "tool": "search_federal_awards",
        "description": "UEI keyword search also works if you have the UEI but not the tool",
        "example": "search_federal_awards('NWM1JWVDA853')"
      }
    },
    "common_uei_examples": {
      "NWM1JWVDA853": {
        "vendor_name": "AM General LLC",
        "primary_customer": "Department of Defense",
        "total_federal_spending": "$4.5B+",
        "note": "Major defense contractor"
      },
      "G2NLP3NLX5Q7": {
        "vendor_name": "Graybar Electric Company, Inc.",
        "note": "Example vendor with government contracts"
      }
    }
  },
