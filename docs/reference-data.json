    },
    "month": {
      "format": "YYYY-MM",
      "example": "2024-01",
      "meaning": "Full calendar month",
      "group_by": "month"
    },
    "note": "Use these formats in response parsing, not in API requests (API requests use YYYY-MM-DD)"
  },
